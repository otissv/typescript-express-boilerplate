{
  "name": "typescript-express-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "start": "node dist",
    "serve": "run-p watch dev",
    "dev": "nodemon dist",
    "watch": "tsc --watch",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:coverage": "npm run build && jest --coverage",
    "test:watch": "jest --watch",
    "docs": "npm run build && typedoc src/index.ts",
    "generate": "npm run clean && mkdir dist  && node ./scripts",
    "clean": "rm -rf ./dist",
    "prepare": "npm run generate",
    "publish": "cd dist && npm publish"
  },
  "author": "Otis Virginie",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "ufunc": "^3.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-disable": "^2.0.1",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  }
}
